{
  "compilerOptions": {
    "target": "ES2017",
    "baseUrl": "./",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@types/*": ["src/types/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/layout/navbar.js",
    "src/components/layout/footer.js"
, "src/components/ProfileSidebar.js" ,
"src", "global.d.ts" ],
  "exclude": [
    "node_modules"
  ]
}